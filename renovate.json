{
  "extends": [
	  "config:base",
	  ":disableRateLimiting"
  ],
  "enabledManagers": [
	  "npm"
  ],
  "rebaseWhen": "behind-base-branch",
  "node": {
	  "supportPolicy": ["lts"]
  },
  "ignoreDeps": ["moment", "moment-timezone"],
  "automerge": true,
  "packageRules": [
	  {
		  "matchPackageNames": ["beeper"],
		  "allowedVersions": "<3.0.0"
	  }
  ]
}
